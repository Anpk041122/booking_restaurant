<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".views.fragment_admins.CatergoryAdminFragment">


   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent"
       android:orientation="vertical">

      <TextView
          android:id="@+id/txtTitleCategoryStory"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:text="@string/title_category_fragment"
          android:fontFamily="@font/lobster"
          android:textSize="30dp"
          android:gravity="center"
          android:padding="10dp"
          />



      <androidx.recyclerview.widget.RecyclerView
          android:id="@+id/recyclerCategoryAdminFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          android:layout_marginBottom="10dp"
          />

      <FrameLayout
          android:id="@+id/fragmentContainerAdmin"
          android:layout_width="match_parent"
          android:layout_height="wrap_content"/>

   </LinearLayout>


</FrameLayout>